{
  "parser": 'babel-eslint',
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "jsx-a11y"
  ],
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }]
  }
}
